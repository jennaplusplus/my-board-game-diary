<%= form_for @meetup, url: @url_path do |f| %>
  <fieldset>
    <legend>Meetup</legend>
      <%= f.label :name, "Name: "%>
      <%= f.text_field :name %>
      <br>
      <%= f.label :location, "Location: "%>
      <%= f.text_field :location %>
      <br>
      <%= f.label :date, "Date: "%>
      <%= f.date_field :date %>
      <br>
      <%= f.submit "#{button_text}" %>
  </fieldset>
<% end %>
